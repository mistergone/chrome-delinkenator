{
  "manifest_version": 2,

  "name": "Disabler",
  "description": "This extension can disable links for GTM testing",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },  

  "background": {
    "scripts": ["background.js", "jquery.min.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.min.js"],
      "css": ["disabler.css"],
      "run_at": "document_end"
    }
  ]

  "permissions": [
    "tabs", "http://*/*"
  ]
}
